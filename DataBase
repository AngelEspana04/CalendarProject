-- DROP TABLE IF EXISTS public.events;

CREATE TABLE IF NOT EXISTS public.events
(
    event_id integer NOT NULL DEFAULT nextval('events_event_id_seq'::regclass),
    event_name character varying(255) COLLATE pg_catalog."default" NOT NULL,
    event_date date NOT NULL,
    event_description text COLLATE pg_catalog."default",
    event_location character varying(255) COLLATE pg_catalog."default",
    CONSTRAINT events_pkey PRIMARY KEY (event_id)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.events
    OWNER to postgres;
